{
    "name": "plural-ripper",
    "version": "0.2.0",
    "description": "A simple executable to download courses from https://pluralsight.com",
    "main": "/bin/main.js",
    "type": "module",
    "bin": {
        "plural-ripper": "/bin/main.js"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "test": "rimraf bin && tsc && node ./bin/main.js",
        "build": "rimraf bin && tsc",
        "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{js,ts,json}\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sayeed205/plural-ripper.git"
    },
    "keywords": [
        "pluralsight",
        "ripper",
        "download",
        "courses",
        "learning",
        "education"
    ],
    "author": {
        "name": "Sayed Ahmed",
        "email": "sayeed205@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sayeed205/plural-ripper/issues"
    },
    "homepage": "https://github.com/sayeed205/plural-ripper#readme",
    "devDependencies": {
        "@types/cli-progress": "^3.11.0",
        "@types/commander": "^2.12.2",
        "@types/figlet": "^1.5.6",
        "@types/inquirer": "^9.0.3",
        "rimraf": "^4.4.1",
        "typescript": "^5.0.2"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "cli-progress": "^3.12.0",
        "commander": "^10.0.0",
        "figlet": "^1.5.2",
        "inquirer": "^9.1.5"
    }
}
